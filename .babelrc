{
  "presets": [
    "env",
    "es2017",
    "stage-3",
    "flow"
  ],
  "plugins": [
    "transform-flow-strip-types",
    "transform-class-properties",
    "transform-runtime",
    [
      "module-resolver",
      {
        "root": [
          "./",
          "./src"
        ]
      }
    ]
  ]
}
